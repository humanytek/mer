<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
        <!-- 06/10/2015 (felix) Form wizard, Products to consume -->
        <record id="view_move_form_inherit_1" model="ir.ui.view">
            <field name="name">stock.move.form.inherit.1</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_form"/>
            <field name="priority" eval="200"/>
            <field name="arch" type="xml">
            
                <!-- 06/10/2015 (felix) Add parameter "context" in onchange_product_id -->
                <xpath expr="//form/sheet/group[1]/group[@name='main_grp']/group[@name='main_grp_col1']/field[@name='product_id']" position="replace">
                    <field name="product_id" on_change="onchange_product_id(product_id, location_id, location_dest_id, False)"/>
                </xpath>
                
            </field>
        </record>
                
    </data>
</openerp>
